{
  "name": "tourze/quic-frames",
  "description": "QUIC协议帧处理库 - 提供各种QUIC帧的编解码和管理",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "tourze/quic-core": "0.0.*",
    "tourze/quic-packets": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\QUIC\\Frames\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\QUIC\\Frames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}